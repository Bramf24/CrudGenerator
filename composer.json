{
    "name": "bramf/crud-generator",
    "type": "library",
    "description": "Crud generator for lumen with Open Api annotations",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Bramf24/CrudGenerator"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bramf\\CrudGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vlad",
            "email": "mun.vladislav.a@gmail.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^3.6",
        "zircote/swagger-php": "^4.7",
        "flipbox/lumen-generator": "^9.2.0",
        "awobaz/compoships": "^2.2.3"
    },
    "scripts": {
        "post-package-update": [
            "Bramf\\CrudGenerator\\Console\\CrudMakeTableCommand::composer"
        ]
    }
}